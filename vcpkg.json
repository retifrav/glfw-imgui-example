{
    "name": "glfw-imgui-example",
    "version": "0",
    "default-features":
    [
        "with-opengl"
    ],
    "features":
    {
        "with-opengl":
        {
            "description": "Using OpenGL as the graphics backend",
            "dependencies":
            [
                "glad",
                "glfw",
                {
                    "name": "dear-imgui",
                    "features":
                    [
                        "backend-glfw-opengl"
                    ]
                }
            ]
        },
        "with-vulkan":
        {
            "description": "Using Vulkan as the graphics backend",
            "dependencies":
            [
                {
                    "name": "dear-imgui",
                    "features":
                    [
                        "backend-glfw-vulkan"
                    ]
                },
                {
                    "name": "glfw",
                    "features":
                    [
                        "vulkan-static"
                    ]
                }
            ]
        }
    },
    "overrides":
    [
        {
            "name": "glad",
            "version": "0.1.36"
        },
        {
            "name": "glfw",
            "version": "3.3.8"
        },
        {
            "name": "dear-imgui",
            "version": "1.89.3"
        }
    ]
}
